#!/usr/bin/env python

import os

print("The script starts here")

x = 10
y = 5


def find_greater(value1, value2):
    if value1 > value2:
        return True
    else:
        return False


if find_greater(x, y):
    print("{} is greater than {}".format(x, y))
else:
    print("{} is greater than {}".format(y, x))


